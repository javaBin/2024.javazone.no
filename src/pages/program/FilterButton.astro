---
import type {HTMLAttributes} from "astro/types";
interface Props extends HTMLAttributes<'button'> {
    dataActive?: boolean;
}
---
    <button {...Astro.props}
        class="bg-darkgreen active:bg-white py-6 px-6 sm:px-10 text-white no-underline rounded-md hover:bg-white hover:border-2 border-darkgray hover:text-darkgray max-sm:text-lg whitespace-nowrap"
        data-active={Astro.props.dataActive}>
        <slot/>
    </button>


